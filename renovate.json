{
  "bumpVersion": "major",
  "prCreation": "not-pending",
  "rangeStrategy": "bump",
  "travis": {
    "enabled": true
  },
  "supportPolicy": [
    "all"
  ],
  "packageRules": [
    {
      "datasources": [
        "docker"
      ],
      "packageNames": [
        "node"
      ]
    },
    {
      "managers": [
        "npm"
      ],
      "depTypeList": [
        "devDependencies"
      ],
      "automerge": true,
      "automergeType": "branch"
    },
    {
      "managers": [
        "npm"
      ],
      "updateTypes": [
        "minor",
        "patch",
        "pin"
      ],
      "automerge": true,
      "automergeType": "branch"
    }
  ]
}